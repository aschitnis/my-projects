<Window x:Class="DCOM.WPF.MVVM.IDialogService.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:DCOM.WPF.MVVM.IDialogService"
        mc:Ignorable="d"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition Height="40"/>
            <RowDefinition Height="40"/>
        </Grid.RowDefinitions>
        <Button VerticalAlignment="Center" Grid.Row="0" HorizontalAlignment="Center" Command="{Binding DisplayDialogWindowCommand}" Content="Display Dialog Window"/>
        <Button VerticalAlignment="Center" Grid.Row="1" HorizontalAlignment="Center" Command="{Binding DisplaySecondDialogWindowCommand}" Content="Display Second Dialog Window"/>
    </Grid>
</Window>
